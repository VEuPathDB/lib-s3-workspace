<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="EndToEnd">

    <Appenders>
        <Console name="STDOUT">
            <PatternLayout pattern="%d{ISO8601} %highlight{[%-5p]} [%24c] %m%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Root level="info">
            <AppenderRef ref="STDOUT" />
        </Root>

        <Logger name="org.veupathdb.lib.s3.workspaces.util" level="trace" additivity="false">
            <AppenderRef ref="STDOUT" />
        </Logger>

        <Logger name="org.veupathdb.lib.s3.workspaces" level="trace" additivity="false">
            <AppenderRef ref="STDOUT" />
        </Logger>

        <Logger name="org.veupathdb.lib.s3.s34k" level="warn" additivity="false">
            <AppenderRef ref="STDOUT" />
        </Logger>
    </Loggers>
</Configuration>
